{
  "name": "fonds-midway",
  "version": "1.0.0",
  "description": "fonds midway components",
  "private": true,
  "packageManager": "pnpm@9.15.2",
  "engines": {
    "node": ">= 18.0.0"
  },
  "scripts": {
    "build": "pnpm -r run build",
    "dev": "pnpm -r --parallel run dev",
    "test": "pnpm -r run test",
    "cov": "pnpm -r run cov --concurrency 2",
    "lint": "pnpm -r exec mwts check",
    "lint:fix": "pnpm -r exec mwts fix",
    "clean": "pnpm -r exec rm -rf dist && rm -rf node_modules",
    "changeset": "changeset",
    "version": "changeset version",
    "release": "rm -f ./packages/.DS* && pnpm build && changeset publish"
  },
  "devDependencies": {
    "@changesets/cli": "^2.27.0",
    "@types/jest": "29.5.14",
    "@types/node": "22.12.0",
    "cross-env": "^7.0.0",
    "jest": "29.7.0",
    "madge": "6.1.0",
    "mwts": "1.3.0",
    "ts-jest": "29.2.5",
    "ts-node": "10.9.2",
    "typedoc": "^0.26.0",
    "typescript": "4.9.5"
  },
  "keywords": [
    "midway",
    "fonds"
  ],
  "license": "MIT"
}
